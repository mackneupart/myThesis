{
  "expo": {
    "name": "Queer Annotations",
    "slug": "queer-annotations",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": ".src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mackneupart.queerannotations",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to be able to function purposes.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs continuous access to your location for ABC reasons.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugin": [
      "expo-camera"
    ],
    "extra": {
      "eas": {
        "projectId": "306381d4-be55-4b5d-b5cd-ac4813cb0466"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/306381d4-be55-4b5d-b5cd-ac4813cb0466"
    }
  }
}